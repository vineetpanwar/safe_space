{
  "version": 2,
  "builds": [
    { "src": "/api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.py" }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python",
      "memory": 1024,
      "maxDuration": 30,
      "includeFiles":  "requirements.txt"
    }
  }
}